{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "api/upload": {
      "maxDuration": 10,
      "memory": 512,
      "maxSize": "100mb"
    }
  }
}
